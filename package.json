{
  "name": "minhaagendaai_v2",
  "private": true,
  "packageManager": "pnpm@10.24.0",
  "scripts": {
    "dev": "turbo run dev --parallel",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "db:generate": "pnpm --filter @repo/db run generate",
    "db:push": "pnpm --filter @repo/db run push",
    "db:smoke": "pnpm --filter @repo/db run smoke",
    "db:seed": "pnpm --filter @repo/db run seed",
    "db:clean:solo": "pnpm --filter @repo/db run clean:solo",
    "db:seed:solo:complete": "pnpm --filter @repo/db run seed:solo:complete",
    "db:migrate:manual": "pnpm --filter @repo/db run migrate:manual",
    "db:migrate:manual:002": "pnpm --filter @repo/db run migrate:manual:002",
    "db:migrate:manual:003": "pnpm --filter @repo/db run migrate:manual:003",
    "db:migrate:manual:005:profile": "pnpm --filter @repo/db run migrate:manual:005:profile",
    "db:migrate:manual:006": "pnpm --filter @repo/db run migrate:manual:006",
    "db:reset": "pnpm --filter @repo/db run reset",
    "mcp:build": "pnpm --filter @repo/mcp-server run build",
    "mcp:dev": "pnpm --filter @repo/mcp-server run dev",
    "mcp:start": "pnpm --filter @repo/mcp-server run start",
    "mcp:debug": "dotenv -e ./.env -- tsx scripts/mcp-client.ts",
    "mcp:debug:vercel": "dotenv -e ./.env -v MCP_CLIENT_TOOL_SOURCE=vercel -- tsx scripts/mcp-client.ts",
    "mcp:debug:mcp": "dotenv -e ./.env -v MCP_CLIENT_TOOL_SOURCE=mcp -- tsx scripts/mcp-client.ts",
    "mcp:chat": "dotenv -e ./.env -- tsx scripts/mcp-chat.ts",
    "diagnose:google": "dotenv -e ./.env -- tsx scripts/diagnose-google-integration.ts",
    "check:integration": "dotenv -e ./.env -- tsx scripts/check-google-integration.ts"
  },
  "devDependencies": {
    "@modelcontextprotocol/sdk": "^1.24.3",
    "@types/node": "^24.10.2",
    "dotenv-cli": "^7.4.4",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@ai-sdk/openai": "^2.0.80",
    "ai": "^5.0.108",
    "dotenv": "^16.4.7",
    "drizzle-kit": "^0.31.8",
    "drizzle-orm": "^0.45.1",
    "openai": "^6.10.0"
  }
}
